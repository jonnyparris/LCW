<p id="notice"><%= notice %></p>
<h4><%= @vendor.name %></h4>
<%= image_tag(@vendor.photos.first.url) if @vendor.photos? %>
<ul>
  <% @vendor.attributes.each do |attribute| %>
    <% if attribute[1] != "" %>
          <li><%= "#{attribute[0]}: #{attribute[1]}" %></li>
        <% end %>
  <% end %>
</ul>
<%= link_to 'Edit', edit_vendor_path(@vendor) %> |
<%= link_to 'Back', vendors_path %>
