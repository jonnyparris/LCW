<%= simple_form_for(@venue) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :contact_details %>
    <%= f.input :website %>
    <%= f.input :pricing %>
    <%= f.input :description %>
    <%= f.input :size_details %>
    <%= f.input :directions %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
